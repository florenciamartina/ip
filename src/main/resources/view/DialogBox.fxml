<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308"
         maxWidth="1.7976931348623157E308"
         prefWidth="400.0" style="-fx-background-color: black;" type="javafx.scene.layout.HBox"
         xmlns="http://javafx.com/javafx/11.0.1">
    <children>
        <Label fx:id="dialog" style="-fx-background-color: #015366;" text="Label" textFill="WHITE" wrapText="true">
            <font>
                <Font name="Libre Baskerville" size="14.0"/>
            </font>
            <padding>
                <Insets bottom="2.0" left="10.0" right="10.0" top="2.0"/>
            </padding>
            <HBox.margin>
                <Insets left="15.0" right="15.0"/>
            </HBox.margin>
        </Label>
        <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true"/>
    </children>
    <padding>
        <Insets bottom="15.0" left="24.0" right="15.0" top="15.0"/>
    </padding>
</fx:root>
